     xml:base="https://w3id.org/pko"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:mod="https://w3id.org/mod#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:bibo="http://purl.org/ontology/bibo/"
     xmlns:dcam="http://purl.org/dc/dcam/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="http://schema.org/">
    <owl:Ontology rdf:about="https://w3id.org/pko">
        <owl:versionIRI rdf:resource="https://w3id.org/pko/1.0.0"/>
        <dc:contributor>Antonia Azzini (Cefriel)</dc:contributor>
        <dc:contributor>Ilaria Baroni (Cefriel)</dc:contributor>
        <dc:contributor>Irene Celino (Cefriel)</dc:contributor>
        <dc:creator>Mario Scrocca (Cefriel)</dc:creator>
        <dc:creator>Valentina Carriero (Cefriel)</dc:creator>
        <dc:title>Procedural Knowledge Ontology (PKO)</dc:title>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <terms:publisher>Cefriel</terms:publisher>
        <bibo:status xml:lang="en">Published Ontology</bibo:status>
        <schema:funding rdf:resource="https://doi.org/10.3030/101120323"/>
        <rdfs:label>Procedural Knowledge Ontology (PKO)</rdfs:label>
        <owl:priorVersion rdf:resource="https://w3id.org/pko/0.1.0"/>
        <owl:versionInfo>1.0.0</owl:versionInfo>
        <mod:createdWith rdf:resource="https://chowlk.linkeddata.es/"/>
    </owl:Ontology>
    


