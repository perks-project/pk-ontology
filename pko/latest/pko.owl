    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms/creator -->


    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/creator">
        <dcam:domainIncludes rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
        <dcam:rangeIncludes rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:label>creator</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Resource (e.g., a Procedure) to the Agent that created it.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/format -->


    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/format">
        <dcam:domainIncludes rdf:resource="http://purl.org/dc/terms/MediaTypeOrExtent"/>
        <dcam:domainIncludes rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
        <rdfs:label>format</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Resource (e.g., a Procedure or a Resource referenced by a Procedure), to the Media Type or Extent of the resource.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/references -->


    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/references">
        <dcam:domainIncludes rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
        <dcam:rangeIncludes rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
        <rdfs:label>references</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Resource (e.g., a Procedure) to another Resource (e.g., a document, an image, or another Procedure) that the Procedure references.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/temporal -->


    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/temporal">
        <dcam:domainIncludes rdf:resource="http://purl.org/spar/pro/RoleInTime"/>
        <dcam:rangeIncludes rdf:resource="http://purl.org/dc/terms/PeriodOfTime"/>
        <rdfs:label>temporal</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Role In Time held by an Agent to the Period of Time in which that Role is valid.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/p-plan#correspondsToVariable -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/p-plan#correspondsToVariable">
        <dcam:domainIncludes rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <dcam:rangeIncludes rdf:resource="http://purl.org/net/p-plan#Variable"/>
        <rdfs:label>corresponds to variable</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link an Entity used by an Activity (either a Procedure Execution or a Step Execution) to the Variable it represented in the Procedure.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/p-plan#hasInputVar -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/p-plan#hasInputVar">
        <dcam:domainIncludes rdf:resource="http://purl.org/net/p-plan#Step"/>
        <dcam:rangeIncludes rdf:resource="http://purl.org/net/p-plan#Variable"/>
        <rdfs:label>has input var</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Step to the Variable that the Step needs to take as input for the Step Execution.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/p-plan#hasOutputVar -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/p-plan#hasOutputVar">
        <dcam:domainIncludes rdf:resource="http://purl.org/net/p-plan#Step"/>
        <dcam:rangeIncludes rdf:resource="http://purl.org/net/p-plan#Variable"/>
        <rdfs:label>has output var</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Step to the Variable that the Step needs to produce as output of the Step Execution.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/p-plan#isStepOfPlan -->


    <owl:ObjectProperty rdf:about="http://purl.org/net/p-plan#isStepOfPlan">
        <owl:inverseOf rdf:resource="https://w3id.org/pko#hasStep"/>
        <dcam:domainIncludes rdf:resource="http://purl.org/net/p-plan#Step"/>
        <dcam:domainIncludes rdf:resource="https://w3id.org/pko#Procedure"/>
        <rdfs:label>is step of plan</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Step to the Procedure it is a step of.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pro/holdsRoleInTime -->


    <owl:ObjectProperty rdf:about="http://purl.org/spar/pro/holdsRoleInTime">
        <dcam:domainIncludes rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <dcam:rangeIncludes rdf:resource="http://purl.org/spar/pro/RoleInTime"/>
        <rdfs:label>holds role in time</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link an Agent to the Role In Time, that is the role that the Agent holds for a certain Period of Time.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pro/relatesTo -->


    <owl:ObjectProperty rdf:about="http://purl.org/spar/pro/relatesTo">
        <dcam:domainIncludes rdf:resource="http://purl.org/spar/pro/RoleInTime"/>
        <dcam:rangeIncludes rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
        <rdfs:label>relates to</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Role In Time, that is the role that an Agent holds for a certain Period of Time, to the Resource (e.g., a Procedure) that the Role relates to.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pro/withRole -->


    <owl:ObjectProperty rdf:about="http://purl.org/spar/pro/withRole">
        <dcam:domainIncludes rdf:resource="http://purl.org/spar/pro/RoleInTime"/>
        <dcam:rangeIncludes rdf:resource="http://purl.org/spar/pro/Role"/>
        <rdfs:label>with role</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Role In Time, that is the role that an Agent holds for a certain Period of Time, to the Role that is held.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2006/time#unitType -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/2006/time#unitType">
        <dcam:domainIncludes rdf:resource="http://www.w3.org/2006/time#Duration"/>
        <dcam:rangeIncludes rdf:resource="http://www.w3.org/2006/time#TemporalUnit"/>
        <rdfs:label>unit type</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Duration of time to the Temporal Unit which provides the precision of a date-time value or scale of a temporal extent.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/adms#status -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/adms#status">
        <dcam:domainIncludes rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
        <dcam:rangeIncludes rdf:resource="https://w3id.org/pko#ProcedureStatus"/>
        <rdfs:label>status</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link either a Procedure or a Procedure Execution to its Status.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/dcat#hasCurrentVersion -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/dcat#hasCurrentVersion">
        <owl:inverseOf rdf:resource="https://w3id.org/pko#isCurrentVersionOf"/>
        <dcam:domainIncludes rdf:resource="https://w3id.org/pko#Procedure"/>
        <dcam:rangeIncludes rdf:resource="https://w3id.org/pko#Procedure"/>
        <rdfs:label>has current version</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Procedure, for which we want to manage version control, to its current version.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/dcat#hasVersion -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/dcat#hasVersion">
        <dcam:domainIncludes rdf:resource="https://w3id.org/pko#Procedure"/>
        <dcam:rangeIncludes rdf:resource="https://w3id.org/pko#Procedure"/>
        <rdfs:label>has version</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Procedure, for which we want to manage version control, to one of its versions.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/dcat#isVersionOf -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/dcat#isVersionOf">
        <dcam:domainIncludes rdf:resource="https://w3id.org/pko#Procedure"/>
        <dcam:rangeIncludes rdf:resource="https://w3id.org/pko#Procedure"/>
        <rdfs:label>is version of</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to a version of a procedure to the Procedure, for which we want to manage version control.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/dcat#previousVersion -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/dcat#previousVersion">
        <owl:inverseOf rdf:resource="https://w3id.org/pko#nextVersion"/>
        <dcam:domainIncludes rdf:resource="https://w3id.org/pko#Procedure"/>
        <dcam:rangeIncludes rdf:resource="https://w3id.org/pko#Procedure"/>
        <rdfs:label>previous version</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a version of a Procedure to its direct previous version of the same Procedure.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#used -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#used">
        <dcam:domainIncludes rdf:resource="https://w3id.org/pko#StepExecution"/>
        <dcam:rangeIncludes rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:label>used</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Step Execution to an Entity that is being used during execution, and that may correspond to an input Variable of the Step.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#wasAssociatedWith -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasAssociatedWith">
        <dcam:domainIncludes rdf:resource="http://www.w3.org/ns/prov#Activity"/>
        <dcam:rangeIncludes rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:label>was associated with</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link an Activity (either a Procedure Execution or a Step Execution) to an Agent that had some role/responsibility in that Activity.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#wasGeneratedBy -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasGeneratedBy">
        <dcam:domainIncludes rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <dcam:rangeIncludes rdf:resource="https://w3id.org/pko#StepExecution"/>
        <rdfs:label>was generated by</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Step Execution to an Entity that was generated during execution, and that may correspond to an output Variable of the Step.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/pko#addressesError -->


    <owl:ObjectProperty rdf:about="https://w3id.org/pko#addressesError">
        <rdfs:domain rdf:resource="http://purl.org/net/p-plan#Step"/>
        <rdfs:range rdf:resource="https://w3id.org/pko#Error"/>
        <rdfs:comment xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a (Multi)Step of a Procedure to the Error it addresses. Such (Multi)Step represents a Fallback procedure.</rdfs:comment>
        <rdfs:label>addresses error</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a (Multi)Step of a Procedure to the Error it addresses. Such (Multi)Step represents a Fallback procedure.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/pko#executesProcedure -->


    <owl:ObjectProperty rdf:about="https://w3id.org/pko#executesProcedure">
        <rdfs:domain rdf:resource="https://w3id.org/pko#ProcedureExecution"/>
        <rdfs:range rdf:resource="https://w3id.org/pko#Procedure"/>
        <rdfs:comment xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Procedure Execution to the Procedure that is being executed.</rdfs:comment>
        <rdfs:label>executes procedure</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Procedure Execution to the Procedure that is being executed.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/pko#executesStep -->


    <owl:ObjectProperty rdf:about="https://w3id.org/pko#executesStep">
        <rdfs:domain rdf:resource="https://w3id.org/pko#StepExecution"/>
        <rdfs:range rdf:resource="http://purl.org/net/p-plan#Step"/>
        <rdfs:comment xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Step Execution to the Step that is being executed.</rdfs:comment>
        <rdfs:label>executes step</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Step Execution to the Step that is being executed.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/pko#hasEncounteredError -->


    <owl:ObjectProperty rdf:about="https://w3id.org/pko#hasEncounteredError">
        <rdfs:domain rdf:resource="https://w3id.org/pko#IssueOccurrence"/>
        <rdfs:range rdf:resource="https://w3id.org/pko#Error"/>
        <rdfs:comment xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link the Occurrence of an Issue to the Error that is being encountered by the Agent executing a Procedure or Step.</rdfs:comment>
        <rdfs:label>has encountered error</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link the Occurrence of an Issue to the Error that is being encountered by the Agent executing a Procedure or Step.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/pko#hasExpectedDuration -->


    <owl:ObjectProperty rdf:about="https://w3id.org/pko#hasExpectedDuration">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/p-plan#Step"/>
                    <rdf:Description rdf:about="https://w3id.org/pko#Procedure"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#Duration"/>
        <rdfs:comment xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link the Occurrence of an Issue to the Error that is being encountered by the Agent executing a Procedure or Step.</rdfs:comment>
        <rdfs:label>has expected duration</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link either a Procedure or a Step to the Duration that is expected for the execution of that Procedure/Step.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/pko#hasFAQCategory -->


    <owl:ObjectProperty rdf:about="https://w3id.org/pko#hasFAQCategory">
        <rdfs:domain rdf:resource="https://w3id.org/pko#FrequentlyAskedQuestion"/>
        <rdfs:range rdf:resource="https://w3id.org/pko#FAQCategory"/>
        <rdfs:comment xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Frequently Asked Question to a Category it belongs to.</rdfs:comment>
        <rdfs:label>has f a q category</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Frequently Asked Question to a Category it belongs to.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/pko#hasFAQTarget -->


    <owl:ObjectProperty rdf:about="https://w3id.org/pko#hasFAQTarget">
        <rdfs:domain rdf:resource="https://w3id.org/pko#FrequentlyAskedQuestion"/>
        <rdfs:range rdf:resource="https://w3id.org/pko#FAQTarget"/>
        <rdfs:comment xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Frequently Asked Question to the Target of such FAQ.</rdfs:comment>
        <rdfs:label>has f a q target</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Frequently Asked Question to the Target of such FAQ.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/pko#hasFallbackStep -->


    <owl:ObjectProperty rdf:about="https://w3id.org/pko#hasFallbackStep">
        <rdfs:domain rdf:resource="https://w3id.org/pko#Procedure"/>
        <rdfs:range rdf:resource="http://purl.org/net/p-plan#Step"/>
        <rdfs:comment xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Procedure to a (Multi)Step that represents a Fallback procedure for a specific Error that can be encountered during execution.</rdfs:comment>
        <rdfs:label>has fallback step</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Procedure to a (Multi)Step that represents a Fallback procedure for a specific Error that can be encountered during execution.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/pko#hasFirstStep -->


    <owl:ObjectProperty rdf:about="https://w3id.org/pko#hasFirstStep">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/p-plan#MultiStep"/>
                    <rdf:Description rdf:about="https://w3id.org/pko#Procedure"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/net/p-plan#Step"/>
        <rdfs:comment xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Procedure or a MultiStep to its first Step.</rdfs:comment>
        <rdfs:label>has first step</rdfs:label>
        <skos:scopeNote xml:lang="en">In the context of the Procedural Knowledge Ontology (PKO), this property is used to link a Procedure or a MultiStep to its first Step.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/pko#hasIssueOccurrence -->


    <owl:ObjectProperty rdf:about="https://w3id.org/pko#hasIssueOccurrence">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Activity"/>
