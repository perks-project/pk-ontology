@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix mod: <https://w3id.org/mod#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pko: <https://w3id.org/pko#> .
@prefix pko-aud: <https://w3id.org/pko/audit#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix vann: <http://purl.org/vocab/vann/> .

dc:title a owl:AnnotationProperty .

dcterms:contributor a owl:AnnotationProperty .

dcterms:creator a owl:AnnotationProperty .

dcterms:issued a owl:AnnotationProperty .

dcterms:license a owl:AnnotationProperty .

dcterms:modified a owl:AnnotationProperty .

dcterms:publisher a owl:AnnotationProperty .

dcterms:references a owl:ObjectProperty ;
    rdfs:label "references" .

bibo:status a owl:AnnotationProperty .

vann:preferredNamespacePrefix a owl:AnnotationProperty .

vann:preferredNamespaceUri a owl:AnnotationProperty .

schema1:funding a owl:AnnotationProperty .

rdfs:label a owl:AnnotationProperty .

owl:versionIRI a owl:AnnotationProperty .

owl:versionInfo a owl:AnnotationProperty .

prov:generatedAtTime a owl:DatatypeProperty ;
    rdfs:label "generated at time" .

prov:wasAssociatedWith a owl:ObjectProperty ;
    rdfs:label "was associated with" .

prov:wasAttributedTo a owl:ObjectProperty ;
    rdfs:label "was attributed to" .

prov:wasGeneratedBy a owl:ObjectProperty ;
    rdfs:label "was generated by" .

pko:ProcedureExecution a owl:Class ;
    rdfs:label "Procedure Execution" .

pko:StepExecution a owl:Class ;
    rdfs:label "Step Execution" .

pko:executesProcedure a owl:ObjectProperty ;
    rdfs:label "executes procedure" .

<https://w3id.org/pko/audit> a owl:Ontology ;
    rdfs:label "Audit module of Procedural Knowledge Ontology (PKO)" ;
    dc:title "Audit module of Procedural Knowledge Ontology (PKO)" ;
    dcterms:contributor <https://orcid.org/0000-0001-9962-7193> ;
    dcterms:creator <https://orcid.org/0000-0002-8235-7331>,
        <https://orcid.org/0000-0003-1427-3723> ;
    dcterms:issued "2025-09-12" ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
    dcterms:modified "2025-09-12" ;
    dcterms:publisher <https://ror.org/046bfkz44> ;
    bibo:status "Draft Ontology"@en ;
    vann:preferredNamespacePrefix "pko-aud" ;
    vann:preferredNamespaceUri "https://w3id.org/pko/audit#" ;
    schema1:funding <https://doi.org/10.3030/101120323> ;
    owl:versionIRI <https://w3id.org/pko/audit/0.0.1> ;
    owl:versionInfo "0.0.1" ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

pko-aud:hasAuditableProcedure a owl:ObjectProperty ;
    rdfs:label "has auditable procedure" ;
    rdfs:domain pko-aud:AuditableSystem ;
    rdfs:range pko-aud:AuditableProcedure .

pko-aud:hasAuditableSystem a owl:ObjectProperty ;
    rdfs:label "has auditable system" ;
    rdfs:domain pko-aud:AuditableProcedure ;
    rdfs:range pko-aud:AuditableSystem .

pko-aud:hasService a owl:ObjectProperty ;
    rdfs:label "has service" ;
    rdfs:domain pko-aud:AuditableSystem ;
    rdfs:range pko-aud:AuditService .

pko-aud:isServiceFor a owl:ObjectProperty ;
    rdfs:label "is service for" ;
    rdfs:domain pko-aud:AuditService ;
    rdfs:range pko-aud:AuditableSystem .

pko-aud:wasCollectedBy a owl:ObjectProperty ;
    rdfs:label "was collected by" ;
    rdfs:domain pko-aud:AuditBundle ;
    rdfs:range pko-aud:AuditService .

prov:Bundle a owl:Class ;
    rdfs:label "Bundle" .

pko:Procedure a owl:Class ;
    rdfs:label "Procedure" .

pko-aud:AuditBundle a owl:Class ;
    rdfs:label "Audit Bundle" ;
    rdfs:subClassOf prov:Bundle .

prov:SoftwareAgent a owl:Class ;
    rdfs:label "Software Agent" .

pko-aud:AuditableProcedure a owl:Class ;
    rdfs:label "Auditable Procedure" ;
    rdfs:subClassOf pko:Procedure .

pko-aud:AuditService a owl:Class ;
    rdfs:label "Audit Service" ;
    rdfs:subClassOf prov:SoftwareAgent .

pko-aud:AuditableSystem a owl:Class ;
    rdfs:label "Auditable System" ;
    rdfs:subClassOf prov:SoftwareAgent .

